# Configuração MinIO para desenvolvimento local
# Copie este arquivo para .env e ajuste conforme necessário

# MinIO Configuration
MINIO_ENDPOINT=http://localhost:9010
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET_NAME=pdf-accessibility
MINIO_USE_SSL=false

# Buckets específicos
PDF_BUCKET=pdf
TEMP_BUCKET=temp

# Configurações de desenvolvimento
DEBUG=true
LOG_LEVEL=INFO

# Timeout para operações (em segundos)
TIMEOUT=300
